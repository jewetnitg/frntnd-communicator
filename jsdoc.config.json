{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": ""
  },
  "plugins": [
    "node_modules/jsdoc-babel"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./build/docs",
    "recurse": true,
    "tutorials": "./docs"
  }
}